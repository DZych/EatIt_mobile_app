<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage
    x:Class="EatIt.SideMenuView"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:EatIt"
    xmlns:views="clr-namespace:EatIt.Views">
    <FlyoutPage.Flyout>
        <pages:SideMenuViewFlyout x:Name="FlyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <views:HomeView />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>